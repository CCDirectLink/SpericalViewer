{
  "name": "SpericalViewer",
  "version": "0.3.4",
  "description": "Viewer for CrossCode content",
  "repository": "git@github.com:CCDirectLink/SpericalViewer.git",
  "main": "app/main.js",
  "author": "CCDirectLink",
  "license": "MIT",
  "scripts": {
    "test": "mocha --recursive unitTest",
    "postinstall": "node postinstall.js",
    "start": "electron .",
    "pack": "build --dir",
    "pack:all": "build --dir --mac --win --linux",
    "pack:mac": "build --dir --mac",
    "pack:win": "build --dir --win",
    "pack:linux": "build --dir --linux",
    "genVersion": "node genVersion.js",
    "dist": "build",
    "dist:all": "build --mac --win --linux",
    "travis": "./node_modules/.bin/mocha --recursive unitTest"
  },
  "build": {
    "appId": "com.electron.ccdirectlink.sperical-viewer",
    "productName": "SpericalViewer",
    "mac": {
      "category": "service"
    },
    "dmg": {
      "title": "SpericalViewer ${version}"
    },
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "electron": "1.6.16",
    "electron-builder": "^19.55.2",
    "electron-packager": "^10.1.1",
    "electron-rebuild": "^1.7.3",
    "chai": "^4.1.2",
    "mocha": "^5.0.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "imagejs": "0.0.9",
    "unzip": "^0.1.11"
  }
}
