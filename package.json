{
  "name": "SpericalViewer",
  "version": "0.3.1",
  "description": "Viewer for CrossCode content",
  "main": "app/main.js",
  "author": "CCDirectLink",
  "license": "MIT",
  "postinstall": "install-app-deps",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "build --dir",
    "pack:all": "build --dir --mac --win --linux",
    "pack:mac": "build --dir --mac",
    "pack:win": "build --dir --win",
    "pack:linux": "build --dir --linux",
    "fixnative": "./node_modules/.bin/electron-rebuild",
    "fixnative:linux": "./node_modules/.bin/electron-rebuild",
    "fixnative:mac": "./node_modules/.bin/electron-rebuild",
    "fixnative:win": ".\\node_modules\\.bin\\electron-rebuild.cmd",
    "genVersion": "node genVersion.js",
    "dist": "build",
    "dist:all": "build --mac --win --linux"
  },
  "build": {
    "appId": "com.electron.ccdirectlink.sperical-viewer",
    "productName": "SpericalViewer",
    "mac": {
      "category": "service"
    },
    "dmg": {
      "title": "SpericalViewer ${version}"
    },
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "electron-builder": "^19.32.2",
    "electron-packager": "^9.1.0",
    "electron": "^1.7.8",
    "electron-rebuild": "^1.6.0"
  },
  "dependencies": {
    "@mcph/lwip": "git+https://github.com/mixer/lwip.git",
    "electron-is-dev": "^0.3.0",
    "unzip2": "^0.2.5"
  }
}
